@if (User.Identity.IsAuthenticated)
{
    <ul class="nav navbar-nav navbar-right">

        <li>@Html.ActionLink("Log Out", "LogOut", "Account", routeValues: null, htmlAttributes: new { id = "??" })</li>
    </ul>
    <ul class="nav navbar-nav navbar-right">
        <li>
            <ul class="navbar-brand">@User.Identity.Name</ul>
        </li>
    </ul>
}
else
{

    <ul class="nav navbar-nav navbar-right">
        <li>@Html.ActionLink("Register", "Register", "Account", routeValues: null, htmlAttributes: new { id = "Register" })</li>
        <li>@Html.ActionLink("Log in", "Login", "Account", routeValues: null, htmlAttributes: new { id = "Login" })</li>
    </ul>

}